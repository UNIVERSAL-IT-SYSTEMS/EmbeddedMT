Import('env')

lib_files = [
    'outputImageSequence.cpp',
	'socketInterface.cpp',
	'kodi.cpp',
	'bufferOutput.cpp'
]

lib = env.Library('outputMethod', lib_files)
installed_lib = env.Install("{libs_dir}".format(libs_dir=env['LIBS_DIR']), lib)
env.Alias('buildLibs', installed_lib)

files = [
	'unittest/SConscript'
]

SConscript(files)
